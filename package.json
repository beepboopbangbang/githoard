{
  "name": "githoard",
  "displayName": "GitHoard",
  "productName": "GitHoard",
  "version": "0.3.2",
  "author": {
    "name": "Jordan Hess",
    "email": "184880+jojobyte@users.noreply.github.com",
    "url": "https://jojobyte.github.com/githoard/"
  },
  "repository": "jojobyte/githoard",
  "description": "Hoard git repositories with ease",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js",
    "build:dir": "node .electron-vue/build.js --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix",
    "release": "standard-version",
    "commit": "git-cz"
  },
  "build": {
    "productName": "GitHoard",
    "appId": "com.jojobyte.githoard",
    "protocols": [
      {
        "name": "GitHoard",
        "role": "Viewer",
        "schemes": [
          "githoard"
        ]
      },
      {
        "name": "com.jojobyte.githoard",
        "schemes": [
          "github-windows",
          "github-mac",
          "sourcetree"
        ]
      }
    ],
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron",
      "node_modules/",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "static/icons/icon.icns"
    },
    "win": {
      "icon": "static/icons/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "icon": "static/icons",
      "executableName": "githoard",
      "synopsis": "Hoard git repos",
      "category": "Development",
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "deb": {
      "depends": [
        "gconf2",
        "gconf-service",
        "libnotify4",
        "libappindicator1",
        "libxtst6",
        "libnss3",
        "libcurl3"
      ]
    },
    "publish": {
      "provider": "github",
      "private": true
    }
  },
  "dependencies": {
    "@nerdwallet/less-plugin-npm-import": "github:nerdwallet/less-plugin-npm-import",
    "async": "^2.5.0",
    "axios": "^0.16.1",
    "bluebird": "^3.5.0",
    "byline": "^5.0.0",
    "conf": "^1.2.0",
    "date-fns": "^1.28.5",
    "dugite": "^1.42.0",
    "electron-context-menu": "^0.9.1",
    "electron-positioner": "^3.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^1.2.0",
    "electron-window-state": "^4.1.1",
    "git-url-parse": "^6.2.2",
    "github": "^9.3.1",
    "lodash": "^4.17.4",
    "nedb": "^1.8.0",
    "nedb-promise": "^2.0.1",
    "rimraf": "^2.6.2",
    "rxjs": "^5.4.3",
    "search-query-parser": "^1.3.0",
    "srp-js": "^0.2.0",
    "vue": "^2.4.3",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.7.0",
    "vue-rx": "^3.4.0",
    "vuex": "^2.4.0",
    "vuex-router-sync": "^4.3.1"
  },
  "devDependencies": {
    "ace-css": "^1.1.0",
    "animate.css": "^3.5.2",
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-vue-app": "^1.3.0",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "basscss": "^8.0.3",
    "basscss-addons": "^1.0.0",
    "cfonts": "^1.1.3",
    "chai": "^4.1.2",
    "chalk": "^1.1.3",
    "colors.css": "^3.0.0",
    "commitizen": "^2.9.6",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.0.0",
    "del": "^2.2.1",
    "devtron": "^1.1.0",
    "electron": "^1.7.6",
    "electron-builder": "^18.8.1",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-rebuild": "^1.6.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-config-vue": "^2.0.2",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-vue": "^3.13.0",
    "eslint-plugin-vue-libs": "^1.2.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.1",
    "geomicons-open": "^3.0.0-beta.2",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.1",
    "json-loader": "^0.5.7",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "loading-svg": "^1.0.0",
    "lodash-webpack-plugin": "^0.11.4",
    "mocha": "^3.5.3",
    "multispinner": "^0.2.1",
    "postcss-color-function": "^4.0.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "precss": "^1.4.0",
    "require-dir": "^0.3.0",
    "spectron": "^3.7.2",
    "standard-version": "^4.2.0",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^12.2.2",
    "vue-style-loader": "^3.0.2",
    "vue-template-compiler": "^2.4.3",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.1",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "false": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
